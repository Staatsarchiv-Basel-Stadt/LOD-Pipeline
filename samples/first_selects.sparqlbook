  {
    "kind": 2,
    "language": "sparql",
    "value": "# from file queries/select_mandate.rq\r\nPREFIX void: <http://rdfs.org/ns/void#>\r\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\r\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\r\nPREFIX schema: <http://schema.org/>\r\nPREFIX dcterms: <http://purl.org/dc/terms/>\r\nPREFIX rico: <https://www.ica.org/standards/RiC/ontology#>\r\n\r\nSELECT ?Signatur ?Titel ?Entstehungszeitraum ?Archivalienart ?enthält ?DLS_Link (?record as ?URI)\r\nWHERE {\r\n  GRAPH <https://ld.bs.ch/graph/ais-metadata> {\r\n    <https://ld.bs.ch/ais/Record/1107046> ^rico:isDirectlyIncludedIn* ?record\r\n    .\r\n    FILTER EXISTS { ?record a rico:Record }\r\n    FILTER REGEX(?Signatur, '^STA Bf 1 A [1-6]-.*')    \r\n    ?record rico:identifier ?Signatur  ;\r\n            rico:title ?Titel ;\r\n            rico:type ?Archivalienart ;\r\n            rdfs:seeAlso ?DLS_Link\r\n    .\r\n    OPTIONAL { ?record rico:scopeAndContent ?enthält ;\r\n                       rico:hasOrHadAllMembersWithAccumulationDate/rico:expressedDate ?Entstehungszeitraum\r\n    . }\r\n  }\r\n}",
    "metadata": {
      "file": "queries/select_mandate.rq"
    }
  }
]