  "main": "index.js",
  "scripts": {
    "start": "mkdir -p target source; barnard59 run --format text/turtle pipelines/staatsarchiv.ttl",
    "clean-nt": "rm -f target/everything.nt && cat target/*.nt | sed '/_:/d' | sed '/<file/d' | sed '/#(No/d' | sed '/NULL/d'  > target/everything.nt",
    "pipeline": "npm start -- --pipeline",
    "gitUpdate": "node shell/git-pull.js"
  },
