    "barnard59-main": "git+https://github.com/zazuko/barnard59-main.git#master",
    "node-fetch": "^2.4.1",
    "rdf-parser-csvw": "git+https://github.com/rdf-ext/rdf-parser-csvw.git#develop",
    "scp2": "^0.5.0",
    "shelljs": "^0.8.3",
    "shelljs-plugin-ssh": "^1.2.1",
    "simple-git": "^1.110.0",
    "ssh2": "^0.8.2",
    "unzipper": "^0.9.10"
  },
